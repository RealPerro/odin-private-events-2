<p style="color: green"><%= notice %></p>

<h1>Awesome Events created by: <%= @user.email%></h1>

<div id="events">
  <% @user.events.each do |event| %>
    <%= render event %>
    <p>
      <%= link_to "Show this event", event %>
    </p>
  <% end %>
</div>

<%= link_to "New event", new_event_path %>